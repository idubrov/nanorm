<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="TEST">
  <resultMap id="PrimitiveTypesBean" class="com.google.code.nanorm.test.beans.PrimitiveTypesBean">
    <result column="ID" property="id" />
    <result column="primByte" property="primByte"/>
    <result column="wrapByte" property="wrapByte"/>
    <result column="primShort" property="primShort"/>
    <result column="wrapShort" property="wrapShort"/>
    <result column="primInt" property="primInt"/>
    <result column="wrapInt" property="wrapInt"/>
    <result column="primLong" property="primLong"/>
    <result column="wrapLong" property="wrapLong"/>
    <result column="primBoolean" property="primBoolean"/>
    <result column="wrapBoolean" property="wrapBoolean"/>
    <result column="primChar" property="primChar" typeHandler="com.google.code.nanorm.test.perf.CharTypeHandler"/>
    <result column="wrapChar" property="wrapChar" typeHandler="com.google.code.nanorm.test.perf.CharTypeHandler"/>
    <result column="primFloat" property="primFloat"/>
    <result column="wrapFloat" property="wrapFloat"/>
    <result column="primDouble" property="primDouble"/>
    <result column="wrapDouble" property="wrapDouble"/>
    <result column="string" property="string"/>
  </resultMap>

  <select id="selectPrimitiveBean" resultMap="PrimitiveTypesBean" parameterClass="int">
    SELECT * FROM primitive WHERE id = #value#
  </select>  
</sqlMap>
