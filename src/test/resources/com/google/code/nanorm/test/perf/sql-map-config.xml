<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE 
  sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" 
  "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
  <transactionManager type="JDBC">
    <property name="SetAutoCommitAllowed" value="false" />
    <dataSource type="SIMPLE">
      <property name="JDBC.Driver" value="org.h2.Driver" />
      <property name="JDBC.ConnectionURL" value="jdbc:h2:mem:" />
      <property name="JDBC.Username" value="sa" />
      <property name="JDBC.Password" value="" />
    </dataSource>
  </transactionManager>
  <sqlMap resource="com/google/code/nanorm/test/perf/TEST_SqlMap.xml" />
</sqlMapConfig>
